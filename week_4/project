import requests
import json
from openpyxl import Workbook

wb = Workbook()
ws = wb.active


url_breweries = 'https://api.openbrewerydb.org/v1/breweries?by_state=alaska'
url_universities = 'http://universities.hipolabs.com/search?name=alaska'
response1 = requests.get(url_breweries)
response2 = requests.get(url_universities)
data1 = response1.json()
data2 = response2.json()
# print(data1)
print(data2)

